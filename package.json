{
  "name": "houmark.com-website",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"wrangler pages dev ./public --ip=* --port 3000 -b ENVIRONMENT=development\"",
    "start": "wrangler pages dev ./public",
    "pages:deploy": "npm run build && wrangler pages deploy --project-name houmark-com --branch dev ./public",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.5.1",
    "@remix-run/cloudflare-pages": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^10.18.0",
    "isbot": "^4.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "satori": "^0.10.11",
    "svg2img": "^1.0.0-beta.2",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240117.0",
    "@remix-run/dev": "^2.5.1",
    "@remix-run/eslint-config": "^2.5.1",
    "@rvxlab/tailwind-plugin-ios-full-height": "^1.1.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animation-delay": "^1.2.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.23.0"
  },
  "engines": {
    "node": ">=16.13"
  },
  "overrides": {
    "@remix-run/cloudflare-pages": {
      "@cloudflare/workers-types": "$@cloudflare/workers-types"
    },
    "@remix-run/cloudflare": {
      "@cloudflare/workers-types": "$@cloudflare/workers-types"
    }
  }
}