{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --no-restart -c \"wrangler pages dev ./public --port 3000 -b --log-level debug ENVIRONMENT=development\"",
    "start": "wrangler pages dev ./public",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.17.1",
    "@remix-run/cloudflare-pages": "^1.17.1",
    "@remix-run/react": "^1.17.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^10.12.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "satori": "^0.10.1",
    "svg2img": "^1.0.0-beta.2",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230518.0",
    "@remix-run/dev": "^1.17.1",
    "@remix-run/eslint-config": "^1.17.1",
    "@rvxlab/tailwind-plugin-ios-full-height": "^1.1.0",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "eslint": "^8.43.0",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-animation-delay": "^1.0.7",
    "typescript": "^5.1.3",
    "wrangler": "^3.1.1"
  },
  "engines": {
    "node": ">=16.13"
  },
  "overrides": {
    "@remix-run/cloudflare-pages": {
      "@cloudflare/workers-types": "$@cloudflare/workers-types"
    },
    "@remix-run/cloudflare": {
      "@cloudflare/workers-types": "$@cloudflare/workers-types"
    }
  }
}
