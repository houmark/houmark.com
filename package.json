{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^1.11.0",
    "@remix-run/cloudflare-pages": "^1.11.0",
    "@remix-run/react": "^1.11.0",
    "cross-env": "^7.0.3",
    "framer-motion": "^8.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@remix-run/dev": "^1.11.0",
    "@remix-run/eslint-config": "^1.11.0",
    "@rvxlab/tailwind-plugin-ios-full-height": "^1.1.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "eslint": "^8.32.0",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.2.4",
    "tailwindcss-animation-delay": "^1.0.7",
    "typescript": "^4.9.4",
    "wrangler": "^2.8.0"
  },
  "engines": {
    "node": ">=16.13"
  },
  "overrides": {
    "@remix-run/cloudflare-pages": {
      "@cloudflare/workers-types": "$@cloudflare/workers-types"
    },
    "@remix-run/cloudflare": {
      "@cloudflare/workers-types": "$@cloudflare/workers-types"
    }
  }
}