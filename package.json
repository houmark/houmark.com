{
  "name": "houmark.com-website",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "postinstall": "npm run typegen",
    "dev": "remix vite:dev --host",
    "build": "remix vite:build",
    "deploy": "npm run build && wrangler pages deploy --project-name houmark-com --branch dev ./build/client",
    "deploy:production": "npm run build && wrangler pages deploy --project-name houmark-com ./build/client",
    "typegen": "wrangler types",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/cloudflare": "2.7.2",
    "@remix-run/cloudflare-pages": "2.7.2",
    "@remix-run/react": "2.7.2",
    "cross-env": "^7.0.3",
    "framer-motion": "^11.0.5",
    "isbot": "^5.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "satori": "^0.10.13",
    "svg2img": "^1.0.0-beta.2",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@remix-run/dev": "2.7.2",
    "@remix-run/eslint-config": "2.7.2",
    "@rvxlab/tailwind-plugin-ios-full-height": "^1.1.0",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.56.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animation-delay": "^1.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.3.1",
    "wrangler": "^3.28.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}