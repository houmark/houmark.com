    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.4.0",
    "@remix-run/cloudflare-pages": "^2.4.0",
    "@remix-run/react": "^2.4.0",
    "cross-env": "^7.0.3",
    "framer-motion": "^10.16.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "satori": "^0.10.11",
    "svg2img": "^1.0.0-beta.2",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@remix-run/dev": "^2.4.0",
    "@remix-run/eslint-config": "^2.4.0",
    "@rvxlab/tailwind-plugin-ios-full-height": "^1.1.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "eslint": "^8.56.0",
    "npm-run-all": "^4.1.5",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "tailwindcss": "^3.4.0",
    "tailwindcss-animation-delay": "^1.2.0",
    "typescript": "^5.3.3",
    "wrangler": "^3.22.0"
  },
  "engines": {
    "node": ">=16.13"
  },
  "overrides": {
